<AlloySmelterRecipes>

  <vanillaFurnaceRecipes enabled="true"/>       
  
  <recipeGroup name="EnderIO"> <!-- Required for EnderIO to function correctly. -->
    
    <recipe name="Electrical Steel" energyCost="12800">
      <input>
        <itemStack oreDictionary="ingotSteel"/>      
        <itemStack oreDictionary="ingotElectrum"/>
      </input>
      <output>
        <itemStack modID="EnderIO" itemName="itemAlloy" itemMeta="0"/> 
      </output>
    </recipe>
    
    <recipe name="Energetic Alloy" energyCost="12800">
      <input>
        <itemStack oreDictionary="ingotRedAlloy"/>
        <itemStack oreDictionary="ingotElectrum"/>
      </input>                  
      <output>
        <itemStack modID="EnderIO" itemName="itemAlloy" itemMeta="1"/> 
      </output>
    </recipe>
    
    <recipe name="Vibrant Alloy" energyCost="12800">
      <input>
        <itemStack modID="EnderIO" itemName="itemAlloy" itemMeta="1"/> <!-- Energetic Alloy -->
        <itemStack oreDictionary="dustEnderPearl"/>
      </input>                       
      <output>
        <itemStack modID="EnderIO" itemName="itemAlloy" itemMeta="2"/> 
      </output>
    </recipe>
    
    <recipe name="Redstone Alloy" energyCost="12800">
      <input>
        <itemStack oreDictionary="ingotRedAlloy"/>
        <itemStack oreDictionary="dustStone"/>
      </input>
      <output>
        <itemStack modID="EnderIO" itemName="itemAlloy" itemMeta="3"/> 
      </output>
    </recipe>
    
    <recipe name="Conductive Iron" energyCost="12800">
      <input>
        <itemStack oreDictionary="ingotRedAlloy"/>
        <itemStack oreDictionary="ingotIron"/>
      </input>
      <output>
        <itemStack modID="EnderIO" itemName="itemAlloy" itemMeta="4"/> 
      </output>
    </recipe>
    
    <recipe name="Pulsating Iron" energyCost="12800">
      <input>
      <itemStack modID="EnderIO" itemName="itemAlloy" itemMeta="4"/> <!-- Conductive Iron -->
      <itemStack oreDictionary="dustEnderPearl"/>
      </input>
      <output>
        <itemStack modID="EnderIO" itemName="itemAlloy" itemMeta="5"/> 
      </output>
    </recipe>
    
    <recipe name="Dark Steel" energyCost="12800">
      <input>
        <itemStack oreDictionary="ingotSteel"/>         
        <itemStack oreDictionary="dustObsidian"/>
      </input>
      <output>
        <itemStack modID="EnderIO" itemName="itemAlloy" itemMeta="6"/> 
      </output>
    </recipe>
    
    <recipe name="Soularium" energyCost="12800">
      <input>
        <itemStack modID="minecraft" itemName="soul_sand"/>
        <itemStack oreDictionary="ingotElectrum"/>     
      </input>                  
      <output>
        <itemStack modID="EnderIO" itemName="itemAlloy" itemMeta="7"/> 
      </output>
    </recipe>
    
    <recipe name="Fused Quartz" energyCost="12800">
      <input>        
        <itemStack modID="minecraft" itemName="quartz" number="4"/>
      </input>            
      <output>
        <itemStack modID="EnderIO" itemName="blockFusedQuartz" itemMeta="0"/> 
      </output>
    </recipe>

    <recipe name="Fused Glass" energyCost="12800">
      <input>
        <itemStack oreDictionary="sand" />
      </input>            
      <output>
        <itemStack modID="EnderIO" itemName="blockFusedQuartz" itemMeta="1"/> 
      </output>
    </recipe>

    <recipe name="Enlightened Fused Quartz" energyCost="12800">
      <input>        
        <itemStack modID="minecraft" itemName="quartz" number="4"/>
        <itemStack modID="minecraft" itemName="glowstone_dust" number="4"/>
      </input>            
      <output>
        <itemStack modID="EnderIO" itemName="blockFusedQuartz" itemMeta="2"/> 
      </output>
    </recipe>

    <recipe name="Enlightened Fused Glass" energyCost="12800">
      <input>
        <itemStack oreDictionary="sand" />
        <itemStack modID="minecraft" itemName="glowstone_dust" number="4"/>
      </input>            
      <output>
        <itemStack modID="EnderIO" itemName="blockFusedQuartz" itemMeta="3"/> 
      </output>
    </recipe>

   <recipe name="Fused Quartz from Block" energyCost="12800">
        <input>        
            <itemStack modID="minecraft" itemName="quartz_block" number="1"/>
        </input>
        <output>
            <itemStack modID="EnderIO" itemName="blockFusedQuartz" itemMeta="0"/> 
        </output>
    </recipe>

    <recipe name="Enlightened Fused Quartz from Block" energyCost="12800">
        <input>
            <itemStack modID="minecraft" itemName="quartz_block" number="1"/>
            <itemStack modID="minecraft" itemName="glowstone_dust" number="4"/>
        </input>
        <output>
            <itemStack modID="EnderIO" itemName="blockFusedQuartz" itemMeta="2"/>
        </output>
    </recipe>
  </recipeGroup>

